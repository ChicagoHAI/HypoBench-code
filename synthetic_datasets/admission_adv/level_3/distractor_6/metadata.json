{
    "task_name": "Student Admission Prediction",
    "task_type": "classification",
    "task_description": "Predict whether a student will be admitted to a university based on their profile. This dataset corresponds to distractor_6 in level_3.",
    "difficulty_level": 3,
    "features": {
        "math": {
            "type": "categorical",
            "description": "Student's grade in Mathematics",
            "possible_values": [
                "A",
                "B",
                "C",
                "D",
                "F"
            ]
        },
        "num_publications": {
            "type": "numerical",
            "description": "Number of publications authored by the student"
        },
        "num_letters": {
            "type": "numerical",
            "description": "Number of recommendation letters submitted"
        },
        "num_activities": {
            "type": "numerical",
            "description": "Number of extracurricular activities participated in"
        },
        "science": {
            "type": "categorical",
            "description": "Student's grade in Science",
            "possible_values": [
                "A",
                "B",
                "C",
                "D",
                "F"
            ]
        },
        "art": {
            "type": "categorical",
            "description": "Student's grade in Art",
            "possible_values": [
                "A",
                "B",
                "C",
                "D",
                "F"
            ]
        }
    },
    "labels": {
        "admission_distractor_6": {
            "type": "binary",
            "values": [
                "admitted",
                "rejected"
            ],
            "description": "Final admission decision"
        }
    },
    "dataset_sizes": {
        "train": 200,
        "validation": 200,
        "test": 200
    },
    "ground_truth_hypotheses": [
        "A student will be admitted if they have an F in Math."
    ],
    "task_properties": {
        "underlying_model": "decision_tree",
        "balanced": true,
        "noise_level": 0,
        "true_num_hypotheses": 1,
        "distractor": 6,
        "depth": null,
        "abstraction": null
    }
}