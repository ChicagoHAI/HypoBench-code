{
    "task_name": "Student Admission Prediction",
    "task_type": "classification",
    "task_description": "Predict whether a student will be admitted to a university based on their profile. This dataset corresponds to noise_20 in level_3.",
    "difficulty_level": 3,
    "features": {
        "math": {
            "type": "categorical",
            "description": "Student's grade in Mathematics",
            "possible_values": [
                "A",
                "B",
                "C",
                "D",
                "F"
            ]
        }
    },
    "labels": {
        "admission_noise_20": {
            "type": "binary",
            "values": [
                "admitted",
                "rejected"
            ],
            "description": "Final admission decision"
        }
    },
    "dataset_sizes": {
        "train": 200,
        "validation": 200,
        "test": 200
    },
    "ground_truth_hypotheses": [
        "A student will be admitted if they have an A in Math."
    ],
    "task_properties": {
        "underlying_model": "decision_tree",
        "balanced": true,
        "noise_level": 20,
        "true_num_hypotheses": 1,
        "distractor": 0,
        "depth": null,
        "abstraction": null
    }
}